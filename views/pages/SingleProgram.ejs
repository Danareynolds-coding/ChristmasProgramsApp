<%- include('../partials/header') %> 
<main class="main" id="main">
  <div class="container">
    <div text-center">
      <h2 class="text-primary mb-3"><%= data.title %></h2>
      <img src="<%= data.posterURL.main %>" alt="image of <%= data.programs.title %>" class="img-fluid mb-3" style="max-width: 300px;">
      <div class="programs-details">
        <h4 class="text-secondary mb-3">Program Details</h4>
        <ul class="list-unstyled">
          <li>rating:<%= data.rating || 'Unknown' %></li>
          <li>animationType: <%= data.animationType %></li> 
          <li>runtime:<%= data.runtime %></li>
          <li>Yr_released:<%= data.yr_released %></li>
          <li>Budget:<%= data.budget %></li>
          <li>grossProfit:<%= data.grossProfit%></li>
          <li>showing:<%= data.showing %></li>
          <li>description:<%= data.description %></li>
          <li>fivePointRating:<%= data.fivePointRating %></li>
        </ul>
      </div>
    </div>
  </div>
</main>

 <%- include('../partials/footer') %>  